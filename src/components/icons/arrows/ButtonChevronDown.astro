---
type Props = {
  fill?: string;
  stroke?: string;
  className?: string;
};

const { fill = "none", stroke = "black", className = "h-8 w-8" } = Astro.props;
---

<svg
  class={className}
  viewBox="0 0 37 36"
  fill={fill}
  xmlns="http://www.w3.org/2000/svg"
>
  <path
    d="M26 15L18.5 22.5L11 15"
    stroke={stroke}
    stroke-width="2"
    stroke-linecap="round"
    stroke-linejoin="round"></path>
</svg>
